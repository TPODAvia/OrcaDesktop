{
  plugins: {
    ros2dds: {
      domain: 1,
      namespace: "/yhs_yhsros2",
      allow: {
        publishers: ["/yhs_yhsros2/rmf/command"],
        // subscribers: ["/chatter"],
        subscribers: ["/yhs_yhsros2/rmf/event", "/yhs_yhsros2/rmf/map_image/compressed", "/yhs_yhsros2/rmf/map_metadata", "/yhs_yhsros2/rmf/state"],
        service_servers: [".*"],
        service_clients: [".*"],
        action_servers: [],
        action_clients: []
      }
    }
  },

  // Zenoh related configurations
  mode: "client",
  connect: {
    endpoints: ["tcp/192.168.196.162:5447"]
  },
  listen: {
    endpoints: []
  },
  scouting: {
    multicast: {
      enabled: true,
      address: "224.0.0.224:5446"
    }
  },
  transport: {
    link: {
      tx: {
        queue: {
          congestion_control: {
            drop: {
              wait_before_drop: 400000
            }
          }
        }
      }
    }
  }
}
