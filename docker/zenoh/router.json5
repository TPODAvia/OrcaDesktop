{
  mode: "router",

  listen: {
    endpoints: ["tcp/0.0.0.0:5447"]
  },

  // optional â€“ change or disable multicast
  // zenohd -c /etc/zenoh/zenohd-5447.json5
  scouting: {
    multicast: {
      enabled: true,
      address: "224.0.0.224:5446"
    }
  },

    transport: {
        link: {
            tx: {
            queue: {
                congestion_control: {
                drop: {
                    // 400000 microseconds = 0.4 seconds
                    wait_before_drop: 400000
                }
                }
            }
            }
        }
    }
}
